@ARTICLE{8105800,
author={Meng, Weizhi and Li, Wenjuan and Su, Chunhua and Zhou, Jianying and Lu, Rongxing},
journal={IEEE Access}, title={Enhancing Trust Management for Wireless Intrusion Detection via Traffic Sampling in the Era of Big Data},
