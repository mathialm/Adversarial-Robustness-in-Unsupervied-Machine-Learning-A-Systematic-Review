@ARTICLE{8025384,
author={Paridari, Kaveh and O’Mahony, Niamh and El-Din Mady, Alie and Chabukswar, Rohan and Boubekeur, Menouer and Sandberg, Henrik},
journal={Proceedings of the IEEE}, title={A Framework for Attack-Resilient Industrial Control Systems: Attack Detection and Controller Reconfiguration},
year={2018},
