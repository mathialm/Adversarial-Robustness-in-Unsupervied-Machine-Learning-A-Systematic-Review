@INPROCEEDINGS{7034689,
author={Cosovan, Doina and Benchea, Razvan and Gavrilut, Dragos},
booktitle={2014 16th International Symposium on Symbolic and Numeric Algorithms for Scientific Computing}, title={A Practical Guide for Detecting the Java Script-Based Malware Using Hidden Markov Models and Linear Classifiers},
