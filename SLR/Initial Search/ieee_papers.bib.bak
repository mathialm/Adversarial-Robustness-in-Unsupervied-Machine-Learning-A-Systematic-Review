@ARTICLE{8747508,
author={Vuppala, Satyanarayana and Mady, Alie El-Din and Kuenzi, Adam},
journal={IEEE Systems Journal}, title={Moving Target Defense Mechanism for Side-Channel Attacks},
year={2020},
volume={14},
number={2},
pages={1810-1819},
abstract={In this paper, we present a side-channel resilient moving target defense mechanism against power-/electromagnetic-based side-channel attacks. Recent countermeasures use fresh rekeying after every encryption/decryption process; this causes major overhead in synchronizing the communicating parties. In contrast to previous work, our mechanism integrates fresh rekeying and masking techniques at an interval, where these techniques are driven by the maximum number of side-channel leakage traces required toward a successful embedded attack. Hence, the mechanism tracks the effect of attacks on the number of traces, and consequently applies rekeying at suitable intervals to reduce the computational/communication overhead, while increasing the attack cost. The mechanism scalability was evaluated against an advanced attack model based on machine learning methods that reduces significantly the number of traces required for a successful attack under masking implementation.},
keywords={Side-channel attacks;Machine learning;Computational modeling;Encryption;Correlation;Attack;cyber-physical system (CPS);machine learning;side channels},
doi={10.1109/JSYST.2019.2922589},
ISSN={1937-9234},
month={June},}
@INPROCEEDINGS{8569911,
