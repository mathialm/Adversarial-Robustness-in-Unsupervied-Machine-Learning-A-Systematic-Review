@INPROCEEDINGS{9297307,
author={Shafee, Ahmed},
booktitle={2020 International Symposium on Networks, Computers and Communications (ISNCC)}, title={Botnets and their detection techniques},
year={2020},
volume={},
number={},
pages={1-6},
abstract={Botnet is considered a dangerous threat that could harm internet users as it makes the compromised host becomes under the control of the botmaster and follow its commands to perform a wide range of malicious activities such as distributed denial of service (DDOS), spamming, identity theft, click fraud, and phishing attacks. Several kinds of botnets exist such as Remote access Trojans/Remote administration tool (RATs) which are malware packages that are designed as advanced persistent threats (APT) for targeted attacks, e.g. stealing sensitive business and personal information or damaging the victim machine. The severity of this kind of bots is that they can evade detection strategies and security countermeasures that are taken by users resulting in serious damages for the infected hosts. Two methods in detection techniques are commonly used either the detection of the botnet at the network level or Identifying botnet behavior at the host level. In network-based detection, the network traffic is captured and analyzed for detecting the infected hosts. On the one hand, when the network traffic is encrypted, the botnet detection becomes tedious. On the other hand, monitoring the network traffic may raise concerns about data privacy. In host-based detection, the system behavior of each host is monitored to detect any botnet-related suspicious behaviors. The main disadvantages of host-based methods are their vulnerability to host-resident and stealthy malware, as well as the difficulties presented in installing the monitored systems in each host in the network. Accordingly, several researches use a hybrid methodology for combining the advantages of both techniques. In this paper, a survey is introduced for the most common botnet types and their detection strategies.},
keywords={Botnet;Protocols;Monitoring;Servers;Tools;Cryptography;Computational modeling;Botnets;intrusion detection;machine learning;network-based detection;host-based detection},
doi={10.1109/ISNCC49221.2020.9297307},
