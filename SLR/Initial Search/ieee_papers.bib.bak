@INPROCEEDINGS{7266849,
author={Zhao, Bin and Liu, Peng},
booktitle={2015 45th Annual IEEE/IFIP International Conference on Dependable Systems and Networks}, title={Private Browsing Mode Not Really That Private: Dealing with Privacy Breach Caused by Browser Extensions},
year={2015},
volume={},
