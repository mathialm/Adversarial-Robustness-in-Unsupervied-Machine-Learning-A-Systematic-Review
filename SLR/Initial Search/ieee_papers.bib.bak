@INPROCEEDINGS{9238838,
author={Zhou, Jie and Luo, Jiangtao and Deng, Lianglang and Wang, Junxia},
booktitle={2020 IEEE/CIC International Conference on Communications in China (ICCC)}, title={Cache Pollution Prevention Mechanism Based on Cache Partition in V-NDN},
year={2020},
volume={},
