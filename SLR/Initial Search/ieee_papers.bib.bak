@INPROCEEDINGS{9155414,
author={Zhang, Xiaoli and Li, Fengting and Zhang, Zeyu and Li, Qi and Wang, Cong and Wu, Jianping},
booktitle={IEEE INFOCOM 2020 - IEEE Conference on Computer Communications}, title={Enabling Execution Assurance of Federated Learning at Untrusted Participants},
year={2020},
