@ARTICLE{6616686,
author={Khattak, Sheharbano and Ramay, Naurin Rasheed and Khan, Kamran Riaz and Syed, Affan A. and Khayam, Syed Ali},
journal={IEEE Communications Surveys Tutorials}, title={A Taxonomy of Botnet Behavior, Detection, and Defense},
year={2014},
volume={16},
