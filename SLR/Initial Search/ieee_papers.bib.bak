@ARTICLE{8718355,
author={Garg, Sahil and Kaur, Kuljeet and Kaddoum, Georges and Ahmed, Syed Hassan and Jayakody, Dushantha Nalin K.},
journal={IEEE Transactions on Vehicular Technology}, title={SDN-Based Secure and Privacy-Preserving Scheme for Vehicular Networks: A 5G Perspective},
year={2019},
