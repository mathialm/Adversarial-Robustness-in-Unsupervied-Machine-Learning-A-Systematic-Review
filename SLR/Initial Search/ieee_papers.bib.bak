@INPROCEEDINGS{9342972,
author={Fernandes, Tiago and Dias, Luis and Correia, Miguel},
booktitle={2020 IEEE 19th International Conference on Trust, Security and Privacy in Computing and Communications (TrustCom)}, title={C2BID: Cluster Change-Based Intrusion Detection},
year={2020},
volume={},
number={},
