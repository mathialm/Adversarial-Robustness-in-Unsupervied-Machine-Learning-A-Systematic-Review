@INPROCEEDINGS{7979810,
author={Koll, David and Schwarzmaier, Martin and Li, Jun and Li, Xiang-Yang and Fu, Xiaoming},
booktitle={2017 IEEE 37th International Conference on Distributed Computing Systems Workshops (ICDCSW)}, title={Thank You For Being A Friend: An Attacker View on Online-Social-Network-Based Sybil Defenses},
year={2017},
volume={},
