@ARTICLE{9415125,
author={Ramakrishna, Raksha and Scaglione, Anna},
journal={IEEE Transactions on Signal Processing}, title={Grid-Graph Signal Processing (Grid-GSP): A Graph Signal Processing Framework for the Power Grid},
year={2021},
volume={69},
