@ARTICLE{8338413,
author={Mai, Guangcan and Cao, Kai and Yuen, Pong C. and Jain, Anil K.},
journal={IEEE Transactions on Pattern Analysis and Machine Intelligence}, title={On the Reconstruction of Face Images from Deep Face Templates},
