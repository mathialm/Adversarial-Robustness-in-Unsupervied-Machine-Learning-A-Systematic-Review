@INPROCEEDINGS{7581460,
author={Abdallah, Walid and Boudriga, Noureddine},
booktitle={2016 22nd Asia-Pacific Conference on Communications (APCC)}, title={A location-aware authentication and key management scheme for wireless sensor networks},
year={2016},
