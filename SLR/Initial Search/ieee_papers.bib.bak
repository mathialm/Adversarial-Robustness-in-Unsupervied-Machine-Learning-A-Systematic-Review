@INPROCEEDINGS{8259829,
author={Sifa, Rafet and Bauckhage, Christian},
booktitle={2017 IEEE International Conference on Data Science and Advanced Analytics (DSAA)}, title={Online k-Maxoids Clustering},
year={2017},
