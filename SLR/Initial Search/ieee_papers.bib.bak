@INPROCEEDINGS{8073834,
author={Chuang, Tzu-Hsien and Huang, Shin-Ying and Mao, Ching-Hao and Jeng, Albert B. and Lee, Hahn-Ming},
booktitle={2017 IEEE Conference on Dependable and Secure Computing}, title={Ziffersystem: A novel malware distribution detection system},
year={2017},
volume={},
