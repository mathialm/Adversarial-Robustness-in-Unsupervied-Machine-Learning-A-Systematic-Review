@INPROCEEDINGS{6523768,
author={Amgai, Ranjit and Shi, Jian and Santos, Renz and Abdelwahed, Sherif},
booktitle={2013 IEEE Electric Ship Technologies Symposium (ESTS)}, title={Machine learning based diagnosis support for ShipBoard Power Systems controls},
year={2013},
volume={},
