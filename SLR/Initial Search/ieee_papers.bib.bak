@INPROCEEDINGS{7431404,
author={Chauvel, Franck and Song, Hui and Fleurey, Franck},
booktitle={2015 IEEE/ACM 8th International Conference on Utility and Cloud Computing (UCC)}, title={Diversity: A Heuristic to Improve Robustness of Self-Adaptive Cloud Architectures},
