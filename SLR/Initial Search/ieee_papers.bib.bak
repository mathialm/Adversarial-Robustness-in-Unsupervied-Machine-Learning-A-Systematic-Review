@INPROCEEDINGS{8839539,
author={Rakin, Adnan Siraj and Fan, Deliang},
booktitle={2019 IEEE Computer Society Annual Symposium on VLSI (ISVLSI)}, title={Defense-Net: Defend Against a Wide Range of Adversarial Attacks through Adversarial Detector},
year={2019},
