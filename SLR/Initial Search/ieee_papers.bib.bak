@ARTICLE{8059780,
author={Kaplan, Emre and Gursoy, Mehmet Emre and Nergiz, Mehmet Ercan and Saygin, Yucel},
journal={IEEE Transactions on Dependable and Secure Computing}, title={Known Sample Attacks on Relation Preserving Data Transformations},
year={2020},
volume={17},
