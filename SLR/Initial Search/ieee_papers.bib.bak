@INPROCEEDINGS{5759431,
author={Hackenberg, Georg and McCall, Rod and Broll, Wolfgang},
booktitle={2011 IEEE Virtual Reality Conference}, title={Lightweight palm and finger tracking for real-time 3D gesture control},
year={2011},
volume={},
