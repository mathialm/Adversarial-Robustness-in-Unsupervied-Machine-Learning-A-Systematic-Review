@INPROCEEDINGS{7546514,
author={Narain, Sashank and Vo-Huu, Triet D. and Block, Kenneth and Noubir, Guevara},
booktitle={2016 IEEE Symposium on Security and Privacy (SP)}, title={Inferring User Routes and Locations Using Zero-Permission Mobile Sensors},
year={2016},
