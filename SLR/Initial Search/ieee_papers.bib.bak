@ARTICLE{8598790,
author={Sedjelmaci, Hichem and Brahmi, Imane Horiya and Ansari, Nirwan and Rehmani, Mubashir Husain},
journal={IEEE Transactions on Emerging Topics in Computing}, title={Cyber Security Framework for Vehicular Network Based on a Hierarchical Game},
year={2021},
