booktitle={IEEE INFOCOM 2019 - IEEE Conference on Computer Communications Workshops (INFOCOM WKSHPS)}, title={Deep Reinforcement Learning for Dynamic Network Slicing in IEEE 802.11 Networks},
year={2019},
volume={},
