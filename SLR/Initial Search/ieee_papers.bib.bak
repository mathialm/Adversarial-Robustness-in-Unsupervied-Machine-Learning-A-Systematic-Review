@ARTICLE{6042389,
author={Cheng, Tsung-Huan and Lin, Ying-Dar and Lai, Yuan-Cheng and Lin, Po-Ching},
journal={IEEE Communications Surveys Tutorials}, title={Evasion Techniques: Sneaking through Your Intrusion Detection/Prevention Systems},
year={2012},
