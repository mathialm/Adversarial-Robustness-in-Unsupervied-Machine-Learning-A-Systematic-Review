@ARTICLE{8852685,
author={Vahidian, Saeed and Hatamnia, Sajad and Champagne, Benoit},
journal={IEEE Access}, title={On the Security Analysis of a Cooperative Incremental Relaying Protocol in the Presence of an Active Eavesdropper},
