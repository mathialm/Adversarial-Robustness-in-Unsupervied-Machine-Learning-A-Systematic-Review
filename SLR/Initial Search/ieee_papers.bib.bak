@INPROCEEDINGS{7916254,
author={Poudyal, Prakash and Goncalves, Teresa and Quaresma, Paulo},
booktitle={2016 10th International Conference on Software, Knowledge, Information Management Applications (SKIMA)}, title={Experiments on identification of argumentative sentences},
year={2016},
volume={},
