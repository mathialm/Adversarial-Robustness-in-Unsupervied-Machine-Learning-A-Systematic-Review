@INPROCEEDINGS{9422049,
author={Zou, Ying and Zhao, Chenglong and Mei, Shibin and Ni, Bingbing},
booktitle={2020 2nd International Conference on Information Technology and Computer Application (ITCA)}, title={Adversarial Defense via Scatter and Alignment},
year={2020},
