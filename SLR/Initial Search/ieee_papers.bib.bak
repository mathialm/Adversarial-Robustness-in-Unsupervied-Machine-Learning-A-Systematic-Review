@INPROCEEDINGS{8854024,
author={Khosroshahi, Yalda and Ozdemir, Enver},
booktitle={2019 6th IEEE International Conference on Cyber Security and Cloud Computing (CSCloud)/ 2019 5th IEEE International Conference on Edge Computing and Scalable Cloud (EdgeCom)}, title={Detection of Sources Being Used in DDoS Attacks},
year={2019},
volume={},
