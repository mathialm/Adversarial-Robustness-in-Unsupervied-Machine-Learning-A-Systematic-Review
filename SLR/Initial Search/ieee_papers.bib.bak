@ARTICLE{7587350,
author={Wu, Jun and Ota, Kaoru and Dong, Mianxiong and Li, Jianhua and Wang, Hongkai},
journal={IEEE Transactions on Big Data}, title={Big Data Analysis-Based Security Situational Awareness for Smart Grid},
