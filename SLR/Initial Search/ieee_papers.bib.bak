@ARTICLE{8735821,
author={Tidjon, Lionel N. and Frappier, Marc and Mammar, Amel},
journal={IEEE Communications Surveys Tutorials}, title={Intrusion Detection Systems: A Cross-Domain Overview},
year={2019},
