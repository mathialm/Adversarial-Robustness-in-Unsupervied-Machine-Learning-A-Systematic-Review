month={April},}
@INPROCEEDINGS{8057195,
author={Achleitner, S. and La Porta, T. and McDaniel, P. and Krishnamurthy, S.V. and Poylisher, A. and Serban, C.},
booktitle={IEEE INFOCOM 2017 - IEEE Conference on Computer Communications}, title={Stealth migration: Hiding virtual machines on the network},
year={2017},
volume={},
