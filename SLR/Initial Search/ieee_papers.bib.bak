@INPROCEEDINGS{8029279,
author={Al-Salah, Tulha and Hong, Liang and Shetty, Sachin},
booktitle={2017 IEEE International Conference on Edge Computing (EDGE)}, title={Attack Surface Expansion Using Decoys to Protect Virtualized Infrastructure},
