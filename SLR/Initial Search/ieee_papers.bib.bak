@INPROCEEDINGS{8288497,
author={Liu, Yi and Li, Ruilin and Liu, Xingtong and Wang, Jian and Tang, Chaojing and Kang, Hongyan},
booktitle={2017 13th International Conference on Computational Intelligence and Security (CIS)}, title={Enhancing Anonymity of Bitcoin Based on Ring Signature Algorithm},
year={2017},
volume={},
