@INPROCEEDINGS{7455824,
author={Liu, Yuan and Dawant, Benoit M.},
booktitle={2016 IEEE-EMBS International Conference on Biomedical and Health Informatics (BHI)}, title={Multi-modal learning-based pre-operative targeting in deep brain stimulation procedures},
year={2016},
volume={},
