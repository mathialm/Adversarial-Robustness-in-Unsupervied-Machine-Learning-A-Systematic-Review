@INPROCEEDINGS{7372954,
author={Vennila, G. and Manikandan, M.S.K and Aswathi, S.},
booktitle={TENCON 2015 - 2015 IEEE Region 10 Conference}, title={Detection of SIP signaling attacks using two-tier fine grained model for VoIP},
year={2015},
volume={},
