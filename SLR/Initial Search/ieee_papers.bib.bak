author={Sattler, Felix and Müller, Klaus-Robert and Wiegand, Thomas and Samek, Wojciech},
booktitle={ICASSP 2020 - 2020 IEEE International Conference on Acoustics, Speech and Signal Processing (ICASSP)}, title={On the Byzantine Robustness of Clustered Federated Learning},
year={2020},
volume={},
