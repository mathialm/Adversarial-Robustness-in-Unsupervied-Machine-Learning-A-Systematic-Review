@INPROCEEDINGS{8487335,
author={Zheng, Zhiyuan and Webb, Allen and Reddy, A. L. Narasimha and Bettati, Riccardo},
booktitle={2018 27th International Conference on Computer Communication and Networks (ICCCN)}, title={IoTAegis: A Scalable Framework to Secure the Internet of Things},
