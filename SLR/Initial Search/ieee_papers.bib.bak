@ARTICLE{7752806,
author={Yang, Xinyu and Zhao, Peng and Zhang, Xialei and Lin, Jie and Yu, Wei},
journal={IEEE Internet of Things Journal}, title={Toward a Gaussian-Mixture Model-Based Detection Scheme Against Data Integrity Attacks in the Smart Grid},
year={2017},
