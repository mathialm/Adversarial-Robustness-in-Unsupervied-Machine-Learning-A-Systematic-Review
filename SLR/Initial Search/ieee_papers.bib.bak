@ARTICLE{9194010,
author={Zhang, Jiale and Chen, Bing and Cheng, Xiang and Binh, Huynh Thi Thanh and Yu, Shui},
journal={IEEE Internet of Things Journal}, title={PoisonGAN: Generative Poisoning Attacks Against Federated Learning in Edge Computing Systems},
year={2021},
