@INPROCEEDINGS{6735689,
author={Ward, Jon R. and Younis, Mohamed},
booktitle={MILCOM 2013 - 2013 IEEE Military Communications Conference}, title={On the Effect of Imperfect Range Estimates on Base Station Anonymity in Wireless Sensor Networks},
year={2013},
volume={},
