@INPROCEEDINGS{6619302,
author={Li, Cheng and Kitani, Kris M.},
booktitle={2013 IEEE Conference on Computer Vision and Pattern Recognition}, title={Pixel-Level Hand Detection in Ego-centric Videos},
year={2013},
