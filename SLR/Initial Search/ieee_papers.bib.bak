@ARTICLE{9339971,
author={Nisioti, Antonia and Loukas, George and Laszka, Aron and Panaousis, Emmanouil},
journal={IEEE Transactions on Information Forensics and Security}, title={Data-Driven Decision Support for Optimizing Cyber Forensic Investigations},
year={2021},
