@INPROCEEDINGS{7966979,
author={Veshchikov, Nikita and Guilley, Sylvain},
booktitle={2017 IEEE European Symposium on Security and Privacy Workshops (EuroS PW)}, title={Use of Simulators for Side-Channel Analysis},
year={2017},
volume={},
