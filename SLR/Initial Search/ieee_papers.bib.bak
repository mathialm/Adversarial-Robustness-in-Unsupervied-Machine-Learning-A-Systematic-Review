@ARTICLE{9367220,
author={Shen, Zhexian and Xu, Kui and Xia, Xiaochen},
journal={IEEE Transactions on Information Forensics and Security}, title={Beam-Domain Anti-Jamming Transmission for Downlink Massive MIMO Systems: A Stackelberg Game Perspective},
year={2021},
