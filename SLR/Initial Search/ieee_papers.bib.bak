@INPROCEEDINGS{9049524,
author={Pontes, Rogério and Maia, Francisco and Vilaça, Ricardo and Machado, Nuno},
booktitle={2019 38th Symposium on Reliable Distributed Systems (SRDS)}, title={d'Artagnan: A Trusted NoSQL Database on Untrusted Clouds},
year={2019},
volume={},
