@INPROCEEDINGS{7382173,
author={Hongyu Sun and Lijun Bi and Binghui Fan and Bisheng Chen and Yinjing Guo},
booktitle={2015 12th International Conference on Fuzzy Systems and Knowledge Discovery (FSKD)}, title={Bhattacharyya distance and confidence map based feature selection for Common spatial patterns algorithms in brain computer interface},
year={2015},
