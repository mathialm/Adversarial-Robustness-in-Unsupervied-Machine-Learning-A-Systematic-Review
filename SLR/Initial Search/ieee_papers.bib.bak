@INPROCEEDINGS{8406613,
author={Papernot, Nicolas and McDaniel, Patrick and Sinha, Arunesh and Wellman, Michael P.},
booktitle={2018 IEEE European Symposium on Security and Privacy (EuroS P)}, title={SoK: Security and Privacy in Machine Learning},
year={2018},
